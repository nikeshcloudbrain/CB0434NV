<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/nativebanner">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <include layout="@layout/native_ad_normal_small" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/imageView4"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_150sdp"
                        android:layout_gravity="center"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginEnd="25dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/ic_se_banner"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                </FrameLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/home"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp">

                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"
                            android:src="@drawable/ic_home" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="Home"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/buy"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp">

                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"
                            android:src="@drawable/ic_buy_credit_score" />


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="Buy Credit Score"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/company"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp">


                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"
                            android:src="@drawable/ic_company_credit_score" />


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="Company Credit Score"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/cal"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp"

                        >

                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"
                            android:src="@drawable/ic_calculator2" />


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="Calculate Score"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dispute"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp"

                        >

                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"

                            android:src="@drawable/ic_dispute_resolution" />


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="Dispute Resolution"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />



                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/center"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp"


                        >

                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"

                            android:src="@drawable/ic_media_center" />


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="Media Center"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/mantri"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp"

                        >

                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"
                            android:src="@drawable/ic_credit_mantri" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="Credit Mantri"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />



                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/faq"
                        android:layout_width="wrap_content"
                        android:layout_height="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/ic_shape"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp"

                        >

                        <ImageView
                            android:layout_width="@dimen/_60sdp"
                            android:layout_height="@dimen/_60sdp"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_10sdp"
                            android:background="@drawable/ic_option_shape"
                            android:padding="14dp"
                            android:src="@drawable/ic_faqs" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/firasans_medium"
                            android:gravity="center"
                            android:text="FAQs"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="20sp" />


                    </LinearLayout>


                </LinearLayout>










            </LinearLayout>
        </LinearLayout>


    </androidx.core.widget.NestedScrollView>

    <include
        android:id="@+id/nativebanner"
        layout="@layout/native_ad_banner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />
</RelativeLayout>